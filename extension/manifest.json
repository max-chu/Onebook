{
    "name": "OneBook",
    "version": "0.0.1",
    "description": "Social Media Organizer",
    "manifest_version": 2,
    "icons": { "48": "assets/icon-48.png",
               "128": "assets/icon-128.png" },
    "background": {
        "scripts": ["background.js"],
        "persistent": false
    },
    "page_action": {
        "default_popup": "popup/contacts/contacts.html"
    },
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "css": ["new_profile.css"],
            "js": ["new_profile.js"]
        }
    ],
    "permissions": ["tabs", "activeTab", "declarativeContent", "storage", "identity"],
    "oauth2": {
        "client_id": "93864762885-7nuqoutvqtrdctkl3kqbctpahl7ph4v0.apps.googleusercontent.com",
        "scopes": ["https://www.googleapis.com/auth/userinfo.email", "https://www.googleapis.com/auth/contacts.readonly"]
    },
    "key": ""
}